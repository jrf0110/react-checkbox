    "url": "https://github.com/jrf0110/react-checkbox/issues"
  },
  "homepage": "https://github.com/jrf0110/react-checkbox",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "less": "^2.5.3",
    "node-sass": "^3.4.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "peerDependencies": {
