{
  "name": "react-checkbox2",
  "version": "1.0.5",
  "description": "Stateless and easily stylable checkbox component for react",
  "main": "dist/react-checkbox.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
